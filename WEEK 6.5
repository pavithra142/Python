def count_frequency(arr):
    frequency = {}
    for num in arr:
        if num in frequency:
            frequency[num] += 1
        else:
            frequency[num] = 1
    return frequency

# Input
n = int(input())
arr = []
for _ in range(n):
    arr.append(int(input()))

# Count frequency
frequency = count_frequency(arr)

# Output
for num, freq in frequency.items():
    print(f"{num} occurs {freq} times")
